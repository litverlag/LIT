function scheduler(){

    var final_date_field = $(".datepicker[name='gprod[final_deadline]']");
    var preps_date_field = $("#preps_deadline");
    var binderei_date_field = $("#binderei_deadline");
    var titelei_date_field = $("#titelei_deadline");
    var umschlag_date_field = $("#umschlag_deadline");
    
	final_date_field.change(alert("GEHT"));

    final_date_field.change(function() {


        var final_date = moment(this.value).format("DD.MM.YYYY");

        var titelei_date = moment(this.value).subtract(parseInt(<%= ChoosableOption.instance.fristen :titelei %>),'days').format("DD.MM.YYYY");
        var umschlag_date = moment(this.value).subtract(parseInt(<%= ChoosableOption.instance.fristen :umschlag %>),'days').format("DD.MM.YYYY");
        var preps_date = moment(this.value).subtract(parseInt(<%= ChoosableOption.instance.fristen :preps %>),'days').format("DD.MM.YYYY");
        var binderei_date = moment(this.value).subtract(parseInt(<%= ChoosableOption.instance.fristen :binderei %>),'days').format("DD.MM.YYYY");


        preps_date_field.val(preps_date);
        binderei_date_field.val(binderei_date);
        titelei_date_field.val(titelei_date);
        umschlag_date_field.val(umschlag_date);



    });
}